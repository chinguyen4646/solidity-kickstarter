{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "object-curly-spacing": ["error", "never"],
        "comma-dangle": ["error", "never"],
        "react/display-name": "off",
        "indent": ["error", 2],
        "react/prop-types": "off"
    }
}
